    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "android": {
      "package": "com.anonymous.saaecaxias"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "4a00c26b-de4e-455e-9458-584f86cba80d"
      }
    }
  }
}
